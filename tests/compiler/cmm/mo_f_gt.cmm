// RUN: "$HC" -cpp -dcmm-lint -keep-s-file -c "$1" && cat "${1%%.*}.s" | FileCheck "$1" -check-prefix=CHECK-A64

#include "Cmm.h"
#include "Types.h"

w64 () {
    // CHECK-A64: fmov d31, xzr
    R1 = %fgt(D1, 0.0 :: W64);

    if (%fgt(D1, 0.0 :: W64)) {
        R2 = 1 :: W64;
    } else {
        R2 = 0 :: W64;
    }
}